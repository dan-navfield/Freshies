{
  "expo": {
    "name": "Freshies",
    "slug": "freshies-app",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1cd3eb13-068b-4669-83b4-ec2f4e1865bf"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "freshies",
    "userInterfaceStyle": "automatic",
    "updates": {
      "enabled": false,
      "url": "https://u.expo.dev/1cd3eb13-068b-4669-83b4-ec2f4e1865bf"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.freshies.app",
      "buildNumber": "6",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Freshies needs access to your photo library to scan product barcodes from existing images.",
        "NSCameraUsageDescription": "Freshies needs camera access to scan product barcodes and take photos of ingredient lists.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.freshies.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Freshies to access your camera to scan product barcodes.",
          "microphonePermission": false
        }
      ],
      [
        "expo-router",
        {
          "origin": false
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
